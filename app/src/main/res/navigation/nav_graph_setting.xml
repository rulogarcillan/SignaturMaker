<?xml version="1.0" encoding="utf-8"?>
<navigation android:id="@+id/nav_graph_setting"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/SettingFragment">

    <fragment
        android:id="@+id/SettingFragment"
        android:name="com.signaturemaker.app.application.features.menu.SettingFragment"
        tools:layout="@layout/setting_fragment">
        <action
            android:id="@+id/action_SettingFragment_to_ChangelogFragment"
            app:destination="@id/ChangelogFragment" />
    </fragment>

    <fragment
        android:id="@+id/ChangelogFragment"
        android:name="com.signaturemaker.app.application.features.menu.changelog.ChangelogFragment"
        tools:layout="@layout/changelog_fragment" />

</navigation>
